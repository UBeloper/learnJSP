		CartmainVO = new CartmainVO();
		
		
		
		System.out.println("====C M====");
		System.out.println(cartmain);
		System.out.println("====C S====");
		System.out.println(cartsub);
		
		/*
		 * mapper.insertMain(cartmain); mapper.insertSub(cartsub);
		 */
		
	}
}
